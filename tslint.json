{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "no-console": false,
      "quotemark": [true, "single"],
      "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
      "indent": [true, "spaces", 2],
      "eofline": true,
      "no-trailing-whitespace": [true],
      "semicolon": [true, "never"]
    },
    "rulesDirectory": []
}
